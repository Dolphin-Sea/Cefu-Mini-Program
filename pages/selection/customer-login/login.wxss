.content {
  margin-top: 20px;
}

.account {
  display: flex;
  flex-direction: row;
  padding-left: 20px;
  padding-top: 20px;
  padding-bottom: 10px;
  width: 90%;
}

.title {
  margin-right: 30px;
  font-weight: bold;
}

.line {
  border: 1px solid #ccc;
  opacity: 0.2;
  width: 90%;
  margin: 0 auto;
}

.see {
  position: absolute;
  right: 20px;
}

.btn {
  width: 90%;
  margin-top: 40px;
  color: #999;
}

.operate {
  display: flex;
  flex-direction: row;
}

.operate view {
  margin: 0 auto;
  margin-top: 40px;
  font-size: 14px;
  color: #333;
}

.login {
  display: flex;
  flex-direction: row;
  margin-top: 60px;
}

.login view {
  margin: 0 auto;
}

.test {
  color: red;
}

.view1 {
  height: 80px;
  width: 80px;
}
